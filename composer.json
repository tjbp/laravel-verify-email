{
    "name": "tjbp/laravel-verify-emails",
    "description": "Verify email addresses in Laravel using the same pattern as password resets.",
    "keywords": ["email", "verify", "verification", "laravel", "illuminate"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Pitcher",
            "email": "github.com@tjbp.net"
        }
    ],
    "support": {
        "issues": "https://github.com/tjbp/laravel-verify-emails/issues",
        "source": "https://github.com/tjbp/laravel-verify-emails"
    },
    "autoload": {
        "psr-4": {
            "LaravelVerifyEmails\\": "src/"
        }
    },
    "require-dev": {
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "^4.8"
    },
    "require": {
        "illuminate/contracts": "^5.0",
        "illuminate/database": "^5.0",
        "illuminate/mail": "^5.0",
        "illuminate/support": "^5.0"
    },
    "suggests": {
        "illuminate/console": "^5.0",
        "illuminate/http": "^5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelVerifyEmails\\Auth\\VerifyEmails\\VerifyEmailServiceProvider::class"
            ]
        }
    }
}
